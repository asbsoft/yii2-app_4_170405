
Options -Indexes
Options FollowSymlinks

<IfModule mod_rewrite.c>
    RewriteEngine On

    RewriteCond %{REQUEST_URI} ^/back/$
    RewriteRule ^(back)/$ /$1 [R=301,L]
    RewriteCond %{REQUEST_URI} ^/back
    RewriteRule ^back(/.+)?$ backend/web/$1 [L,PT]

    RewriteCond %{REQUEST_URI} ^.*$
    RewriteRule ^(.*)$ frontend/web/$1
</IfModule>


